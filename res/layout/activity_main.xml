<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/topest"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#ffffff"
    android:orientation="vertical" >

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="#ffffff"
        android:gravity="top|bottom"
        android:tag="5"
        android:orientation="horizontal"
        android:paddingBottom="5dp"
        android:paddingLeft="0dp"
        android:paddingTop="5dp" >


       	<com.example.thirdo.GateContainer            
            android:id="@+id/topCont"
            android:layout_width="wrap_content"
            android:orientation="horizontal"
            android:layout_height="match_parent" >
        </com.example.thirdo.GateContainer>
        <Spinner
            android:id="@+id/spinner1"
            android:layout_width="140dp"
            android:layout_height="@dimen/gateDim"
            android:layout_gravity="right"
            android:entries="@array/qubits"
            android:gravity="center" />

        <com.example.thirdo.GateContainer            
            android:id="@+id/compiledGates"
            android:layout_width="wrap_content"
            android:orientation="horizontal"
            android:layout_height="match_parent" >
        </com.example.thirdo.GateContainer>

        <Button
            android:id="@+id/imageButton1"
            style="?android:attr/buttonStyleSmall"
            android:layout_width="200dp"
            android:layout_height="70dp"
            android:onClick="evaluate"
            android:text="@string/eval" />
		 <Button
            android:id="@+id/imageButton2"
            style="?android:attr/buttonStyleSmall"
            android:layout_width="wrap_content"
            android:layout_height="70dp"
            android:onClick="compile"
            android:visibility="invisible"
            android:text="@string/comp" />

    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="0dip"
        android:layout_weight="0.74"
        android:alignmentMode="alignBounds"
        android:orientation="horizontal" >
        
        <LinearLayout
            android:id="@+id/buttonBar"
            android:layout_width="@dimen/gateDim"
            android:layout_height="411dp"
            android:layout_gravity="fill_horizontal"
            android:background="#ffffff"
            android:orientation="vertical" >

            <ImageView
                android:id="@+id/qubitButton1"
                android:layout_width="@dimen/gateDim"
                android:layout_height="@dimen/gateDim"
                android:onClick="buttonClick"
                android:src="@drawable/q0"
            	android:contentDescription="@string/desc" />

            <ImageView
                android:id="@+id/qubitButton2"
                android:layout_width="@dimen/gateDim"
                android:layout_height="@dimen/gateDim"
                android:onClick="buttonClick"
                android:src="@drawable/q0"
            	android:contentDescription="@string/desc" />

            <ImageView
                android:id="@+id/qubitButton3"
                android:layout_width="@dimen/gateDim"
                android:layout_height="@dimen/gateDim"
                android:onClick="buttonClick"
                android:src="@drawable/q0"
            	android:contentDescription="@string/desc"  />

            <ImageView
                android:id="@+id/qubitButton4"
                android:layout_width="@dimen/gateDim"
                android:layout_height="@dimen/gateDim"
                android:onClick="buttonClick"
                android:src="@drawable/q0"
            	android:contentDescription="@string/desc"  />
            <ImageView
                android:id="@+id/qubitButton5"
                android:layout_width="@dimen/gateDim"
                android:layout_height="@dimen/gateDim"
                android:onClick="buttonClick"
                android:src="@drawable/q0"
            	android:contentDescription="@string/desc"  />
            <ImageView
                android:id="@+id/qubitButton6"
                android:layout_width="@dimen/gateDim"
                android:layout_height="@dimen/gateDim"
                android:onClick="buttonClick"
                android:src="@drawable/q0"
            	android:contentDescription="@string/desc"  />
        </LinearLayout>

        <HorizontalScrollView
            android:id="@+id/horizontalScrollView1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" >

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:orientation="vertical" >
           

            <com.example.thirdo.GateContainer
                android:id="@+id/bottom_container"
                android:layout_width="wrap_content"
                android:layout_height="@dimen/gateDim"
                android:background="#FFFFFF"
                android:orientation="horizontal"
                android:tag="0" >
            </com.example.thirdo.GateContainer>

            <com.example.thirdo.GateContainer
                android:id="@+id/bottom_container2"
                android:layout_width="wrap_content"
                android:layout_height="@dimen/gateDim"
                android:background="#FFFFFF"
                android:orientation="horizontal"
                android:tag="1" >
            </com.example.thirdo.GateContainer>

            <com.example.thirdo.GateContainer
                android:id="@+id/bottom_container3"
                android:layout_width="wrap_content"
                android:layout_height="@dimen/gateDim"
                android:background="#FFFFFF"
                android:orientation="horizontal"
                android:tag="2" >
            </com.example.thirdo.GateContainer>

            <com.example.thirdo.GateContainer
                android:id="@+id/bottom_container4"
                android:layout_width="wrap_content"
                android:layout_height="@dimen/gateDim"
                android:background="#FFFFFF"
                android:orientation="horizontal"
                android:tag="3" >
            </com.example.thirdo.GateContainer>

            <com.example.thirdo.GateContainer
                android:id="@+id/bottom_container5"
                android:layout_width="wrap_content"
                android:layout_height="@dimen/gateDim"
                android:background="#FFFFFF"
                android:orientation="horizontal"
                android:tag="4" >
            </com.example.thirdo.GateContainer>

            <com.example.thirdo.GateContainer
                android:id="@+id/bottom_container6"
                android:layout_width="wrap_content"
                android:layout_height="@dimen/gateDim"
                android:background="#FFFFFF"
                android:orientation="horizontal"
                android:tag="5" >
            </com.example.thirdo.GateContainer>
             </LinearLayout>
        </HorizontalScrollView>

    </LinearLayout>

</LinearLayout>